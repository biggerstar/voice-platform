{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@vben/tsconfig/web-app.json",
  "compilerOptions": {
    "declaration": true,
    "lib": [
      "ESNext",
      "DOM" 
    ],
    "target": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": false,
    "baseUrl": ".",
    "paths": {
      "#/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src/env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "vite.config.mts",
  ],
}
